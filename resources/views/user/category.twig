{% extends 'templates/user/profile_template.twig' %}

{% block pageTitle %}{{ user.username }}'s Lists: {{ category }}{% endblock %}

{% block profile_content %}

    <div class="col span_4_of_4">
        <h3><a href="{{ path_for('userProfile', {user : user.username}) }}" class="muted"><span class="icon ion-arrow-left-c"></span> Return to profile</a></h3>
        <h1>Lists: {{ category }}</h1>

        {% for list in lists %}
            {% include 'templates/list/list_template.twig' %}
        {% endfor %}
    </div>

{% endblock %}